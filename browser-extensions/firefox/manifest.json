{
  "manifest_version": 2,
  "name": "Gingko-Codemirror",
  "version": "0.3.4",
  "description": "",
  "icons": {
    "48": "icons/border-48.png"
  },
  "permissions": [
    "*://*.gingkoapp.com/*"
  ],
  "browser_action": {
    "default_icon": "icons/border-48.png",
    "default_popup": "popup/popup.html"
  },
  "web_accessible_resources": [
    "dependencies.json"
  ],
  "content_scripts": [
    {
      "run_at": "document_start",
      "matches": [
        "*://*.gingkoapp.com/*"
      ],
      "js": [
        "codemirror_base.js",
        "codemirror_extended.js",
        "start.js"
      ]
    }
  ]
}
